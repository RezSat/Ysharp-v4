class error {
  func --init--(this, type) {
    this::type = type;
  }

  func raise(this, msg, ln="?", file="?") {
    `errors.ErrorClass(var.this.objects["type"]).raise_error(var.msg.value, file=var.file.value, ln=var.ln.value)`;
  }
}
